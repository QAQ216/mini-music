
.container {
	padding: 20px;
	font-size: 14px;
	line-height: 24px;
}

/* .search {
	
} */
.clearfix:after {
	content: "";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
.clearfix {
	zoom: 1;
}
.search-input {
	width: 85%;
	height: 50rpx;
	text-align: center;
	float: left;
	background-color: #eee;
	border-radius: 50rpx;
}
.search-img {
	height: 50rpx;
	width: 50rpx;
	border-radius: 50%;
	float: right;
}
.banner {
	margin-top: 30rpx;
	border-radius: 20rpx;
}
.banner image {
	width: 100%;
	height: 100%;
	border-radius: 20rpx;
}
.func {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	margin-top: 30rpx;
	/* border: 1rpx solid red; */
	text-align: center;
}
.item-img {
	width: 80rpx;
	height: 80rpx;
	/* border: 1rpx solid red; */
	background-color: red;
	border-radius: 50%;
}
.item-img image {
	width: 60%;
	height: 60%;
	position: relative;
	top: 50%;
	/* left: 50%; */
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}
